namespace Protocol;

table NewBlock
{
    //Filename:string;
    //Order:int;         //块序号
    Offset:long;         //块偏移
    Length:int;          //块长度
    Md5:string;          //二级签名
    Data:string;         //数据
}

table NewBlockAck
{
    SrcPath:string;         //源主机文件路径和名称(请求同步方，持有不完整文件)
    DesPath:string;         //目标主机文件路径和名称(持有完整文件方)
    Splitsize:uint;         //分块大小
    Length:ulong;           //文件总长度
    Infos:NewBlock;         //块信息
}

root_type NewBlockAck;